<h1>Climbs</h1>
<ul>
    <% @climbs.each do |climb| %>
        <% if climb.top_rope %>
            <li><strong><%= climb.name %></strong></li>
            <ul>
                <li>Created At: <%= climb.created_at %></li>
            </ul>
        <% end %>
    <% end %>
</ul>